<Page x:Class="demo3.Pages.qwe"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:demo3.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="qwe">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="13*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <TextBlock VerticalAlignment="Center" Margin="10 0 10 0">
     Список Партнер
            </TextBlock>
            <Button Margin="5" x:Name="goback" Click="goback_Click">назад</Button>
            <TextBlock VerticalAlignment="Center" Margin="10 0 10 0">
                Партнер
            </TextBlock>
            <ComboBox DisplayMemberPath="name" Margin="5" Width="200" Name="partnerCb" SelectionChanged="partnerCb_SelectionChanged"></ComboBox>
        </StackPanel>
        <DataGrid x:Name="namesss" Grid.Row="2">

        </DataGrid>

    </Grid>
</Page>
